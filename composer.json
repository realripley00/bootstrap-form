{
    "name": "realripley00/bootstrap-4-form",
    "description": "Laravel 5 form wrappers for Bootstrap 4.",
    "keywords": [
        "laravel",
        "form",
        "bootstrap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "RealRipley00",
            "email": "realripley00@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/config": "^6.0",
        "illuminate/session": "^6.0",
        "illuminate/support": "^6.0",
        "laravelcollective/html": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "RealRipley\\BootstrapForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RealRipley\\BootstrapForm\\BootstrapFormServiceProvider"
            ],
            "aliases": {
                "BootForm": "RealRipley\\BootstrapForm\\Facades\\BootstrapForm"
            }
        }
    },
    "minimum-stability": "dev"
}